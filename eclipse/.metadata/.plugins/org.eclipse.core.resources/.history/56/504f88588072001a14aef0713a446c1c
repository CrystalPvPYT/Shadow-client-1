		return "Changes speed mode";
	}

	@Override
	public String getSyntax() {
		return ".toggle <mode>";
	}

	@Override
	public void onCommand(String command, String[] args) throws Exception {		
		if(args.length != 1) {
			mess
		}
	}

}
