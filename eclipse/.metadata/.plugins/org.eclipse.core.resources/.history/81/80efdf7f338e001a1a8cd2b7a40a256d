package mystra.module.mods.other;

import mystra.Mystra;
import mystra.module.Category;
import mystra.module.Module;

public class Panic extends Module{

	public Panic() {
		super("Panic", 0, Category.OTHER);
	}
	
	@Override
	public void onDisable() {
		super.onDisable();
	}
	
	@Override
	public void onEnable() {
		for(Module m : Mystra.modulemanager.getModules())
			if(m.isToggled() && m != this)
				m.setToggled(false);
				setState(false);
		super.onEnable();
	}
}
