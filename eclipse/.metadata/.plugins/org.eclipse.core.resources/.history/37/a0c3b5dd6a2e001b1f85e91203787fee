package me.shadow.module.render;

import me.shadow.Shadow;
import me.shadow.clickgui.settings.Setting;
import me.shadow.module.Category;
import me.shadow.module.Module;

public class Ambiance extends Module {

	public Ambiance() {
		super("Ambiance", 0, Category.RENDER);
		
		Shadow.instance.settingsManager.rSetting(new Setting("Time", this, 16000.0D, 1.0D, 24000.0D, false));
	}

}
