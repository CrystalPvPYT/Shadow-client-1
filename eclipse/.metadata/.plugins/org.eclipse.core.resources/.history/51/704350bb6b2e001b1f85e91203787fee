package me.shadow.module.movement;

import me.shadow.event.EventTarget;
import me.shadow.event.events.EventUpdate;
import me.shadow.module.Category;
import me.shadow.module.Module;

public class AutoWalk extends Module{
	public AutoWalk() {
		super("AutoWalk", 0, Category.MOVEMENT);
	}

	@EventTarget
    public void onUpdate(EventUpdate event){
        mc.gameSettings.keyBindForward.pressed = true;
    }
    @Override
    public void onDisable(){
        super.onDisable();
    }

    @Override
    public void onEnable(){
        super.onEnable();
    }
}
